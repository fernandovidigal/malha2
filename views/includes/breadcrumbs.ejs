<% if(typeof breadcrumbs != 'undefined'){ %>
    <ul class="breadcrumbs">
        <% breadcrumbs.forEach((breadcrumb, index) => { %>
            <% if(index < breadcrumbs.length - 1) { %>
                <li class="breadcrumbs__item"><a href="<%= breadcrumb.url %>" class="breadcrumbs__link"><%- (index != 0) ? breadcrumb.name : '<i class="fas fa-home"></i>' %></a></li>
            <% } else { %>
                <li class="breadcrumbs__item breadcrumbs__item__selected"><%= breadcrumb.name %></li>
            <% } %>
        <% }); %>
    </ul>
<% } %>